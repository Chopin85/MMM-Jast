{% macro render() %}
  {% if config.showDepotGrowth %}
    {% set itemCount = config.stocks.length + utils.getDepotGrowth(config, stocks).length %}
  {% else %}
    {% set itemCount = config.stocks.length %}
  {% endif %}
  {% set percentPerItem = 100 / itemCount %}
  <style type="text/css">
  @keyframes tickerv {
  {% for i in range(0, itemCount) -%}
    {{ i *  percentPerItem }}% {
      margin-top: calc(var(--size) * -1.44 * {{ i }});
    }
  {%- endfor %}
  }
  </style>
{% endmacro %}
